{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "lib": [
      "ESNext",
      "DOM"
    ],
    "outDir": "../priv/static/",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "declaration": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitThis": false,
    "skipLibCheck": true,
    "strict": true
  },
  "include": [
    "**/*.ts"
  ]
}
